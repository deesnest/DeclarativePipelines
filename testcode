pipeline {
    agent {label "Agent C"}
    stages {
        stage('build1') {
            steps {
                sh '''
                     sudo su -
                     cd /home/ubuntu/workspace/cproject'
                     gcc -c main.c fact.c palin.c rev.c sum.c'
                     gcc -o ABC.exe main.o fact.o palin.o rev.o sum.o'
                     ./ABC.exe'''
            }
        }
        
        stage('build2') {
            steps {
              sh 'echo "successful"'
            }
        }
    }
}
